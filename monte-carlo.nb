  3.6832852307415457`*^9}},ExpressionUUID->"59ff8036-2fd5-40da-bc0e-\
1bc7e1499883"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x0", "=", 
   RowBox[{"-", "2"}]}], ";", 
  RowBox[{"xf", "=", "2"}], ";", 
  RowBox[{"dx", "=", "0.1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x", "=", 
   RowBox[{"Range", "[", 
    RowBox[{"x0", ",", "xf", ",", "dx"}], "]"}]}], ";", 
  RowBox[{"lx", "=", 
   RowBox[{"Length", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"v", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"1000000.", ",", "lx"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "1"}], "<", 
       RowBox[{"x", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
       "<", "1"}], ",", 
      RowBox[{
       RowBox[{"v", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
       "=", "0.0"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", "0", ",", "lx"}], "}"}]}], "]"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"Construct", " ", "an", " ", "initial", " ", "wavefunction"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Psi]", "=", 
   RowBox[{"0", "*", "x"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "1"}], "\[LessEqual]", 
       RowBox[{"x", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
       "\[LessEqual]", "1"}], ",", 
      RowBox[{
       RowBox[{
       "\[Psi]", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "=", 
       "1"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", "1", ",", "lx"}], "}"}]}], "]"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"Construct", " ", "Hamiltonian"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"H", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{
     RowBox[{"0", "*", "x"}], ",", "lx"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"H", "\[LeftDoubleBracket]", 
    RowBox[{"1", ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", "2"}], "}"}]}], "\[RightDoubleBracket]"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["dx", "2"]], "+", 
      RowBox[{"v", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
     ",", 
     FractionBox[
      RowBox[{"-", "1"}], 
      RowBox[{"2", 
       SuperscriptBox["dx", "2"]}]]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"H", "\[LeftDoubleBracket]", 
    RowBox[{"lx", ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"lx", "-", "1"}], ",", "lx"}], "}"}]}], 
    "\[RightDoubleBracket]"}], " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "1"}], 
      RowBox[{"2", 
       SuperscriptBox["dx", "2"]}]], ",", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["dx", "2"]], "+", 
      RowBox[{
      "v", "\[LeftDoubleBracket]", "lx", "\[RightDoubleBracket]"}]}]}], 
    "}"}]}], ";"}], "\n", 
 RowBox[{"Do", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"H", "\[LeftDoubleBracket]", 
     RowBox[{"i", ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"i", "-", "1"}], ",", "i", ",", 
        RowBox[{"i", "+", "1"}]}], "}"}]}], "\[RightDoubleBracket]"}], "=", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["dx", "2"]], 
     RowBox[{"{", 
      RowBox[{
       FractionBox[
        RowBox[{"-", "1"}], "2"], ",", 
       RowBox[{"1", "+", 
        RowBox[{
         SuperscriptBox["dx", "2"], 
         RowBox[{
         "v", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}]}], ",", 
       FractionBox[
        RowBox[{"-", "1"}], "2"]}], "}"}]}]}], ",", 
   RowBox[{"{", 
    RowBox[{"i", ",", "2", ",", 
     RowBox[{"lx", "-", "1"}]}], "}"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]", "=", 
    FractionBox[
     RowBox[{"\[Psi]", ".", "H", ".", "\[Psi]"}], 
     RowBox[{"\[Psi]", ".", "\[Psi]"}]]}], ";"}], "|", "\[IndentingNewLine]", 
  
  RowBox[{"(*", 
   RowBox[{"Monte", "-", 
    RowBox[{"Carlo", " ", "Algorithm"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"Do", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"ran", "=", 
      RowBox[{"RandomInteger", "[", 
       RowBox[{"{", 
        RowBox[{"1", ",", "lx"}], "}"}], "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"\[Psi]trial", " ", "=", " ", "\[Psi]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
      "\[Psi]trial", "\[LeftDoubleBracket]", "ran", "\[RightDoubleBracket]"}],
       "+=", 
      RowBox[{"RandomReal", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "0.2"}], ",", "0.2"}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Epsilon]new", " ", "=", 
      FractionBox[
       RowBox[{"\[Psi]trial", ".", "H", ".", "\[Psi]trial"}], 
       RowBox[{"\[Psi]trial", ".", "\[Psi]trial"}]]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"\[Epsilon]new", "<", "\[Epsilon]"}], ",", 
       RowBox[{
        RowBox[{"\[Psi]", "=", 
         FractionBox["\[Psi]trial", 
          SqrtBox[
           RowBox[{"dx", " ", 
            RowBox[{"\[Psi]trial", ".", "\[Psi]trial"}]}]]]}], ";", 
        RowBox[{"\[Epsilon]", "=", "\[Epsilon]new"}]}]}], "]"}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"i", ",", "1", ",", "1000000"}], "}"}]}], 
   "]"}]}], "\[IndentingNewLine]", "\[Epsilon]"}], "Input",
 CellChangeTimes->{{3.85057505090904*^9, 3.850575130946891*^9}, {
  3.850575162305752*^9, 3.8505751988573647`*^9}, {3.8505752422250223`*^9, 
